{
  "expo": {
    "name": "Rhema",
    "slug": "Rhema",
    "version": "0.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/**"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.geniusofdigital.rhema"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.geniusofdigital.rhema"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8abe8122-bbc0-4010-b17b-d8bcfe4af782"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["html"],
      "assetExts" : ["html"]
    },
    "updates": {
      "url": "https://u.expo.dev/8abe8122-bbc0-4010-b17b-d8bcfe4af782"
    }
  }
}
